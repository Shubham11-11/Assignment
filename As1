
// Assignment 1
// Output of these snippet of code
// 1

function foo() {
    x = 1;
    function x() { }
    var x = '2';
    return x;
}
console.log( typeof foo() );

// output will  be 'string'



// 2

function foo() {
    return x;
    x = 1;
    
    function x() { }
    
    var x = '2';
}
console.log( typeof foo() ); 

// output will be function 



// 3

var x = 1;
function foo() {
    x = 10;
    return;
    function x() {}
}
foo();
console.log( x );

// output will be 1 